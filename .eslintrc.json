{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": 0,
        "comma-dangle": 0,
        "no-console": 0,
        "linebreak-style": 0,
        "no-extraneous-dependencies": 0,
        "object-curly-newline": 0,
        "max-len": 0
    }
}
